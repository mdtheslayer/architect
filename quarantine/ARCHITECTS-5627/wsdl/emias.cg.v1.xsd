    <xs:simpleType name="ComparisonOperationType">
        <xs:annotation>
            <xs:documentation>Оператор сравнения</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="GT">
                <xs:annotation>
                    <xs:documentation>Условие "больше чем"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GE">
                <xs:annotation>
                    <xs:documentation>Условие "больше или равно"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="EQ">
                <xs:annotation>
                    <xs:documentation>Условие "в точности равно"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="LT">
                <xs:annotation>
                    <xs:documentation>Условие "меньше чем"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="LE">
                <xs:annotation>
                    <xs:documentation>Условие "меньше или равно"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RANGE">
                <xs:annotation>
                    <xs:documentation>Диапазон</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NE">
                <xs:annotation>
                    <xs:documentation>Условие "не равно"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

