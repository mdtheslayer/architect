            <xs:enumeration value="ADD">
                <xs:annotation>
                    <xs:documentation>Добавление</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="REMOVE">
                <xs:annotation>
                    <xs:documentation>Удаление</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="MEDICAL">
                <xs:annotation>
                    <xs:documentation>Событие</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RANGE">
                <xs:annotation>
                    <xs:documentation>Диапазон</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
